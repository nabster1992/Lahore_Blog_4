{
    "builds": [
      {
        "src": "Lahore_Blog/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9",
          "buildCommand": "python manage.py collectstatic --noinput --clear && mv staticfiles staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "Lahore_Blog/wsgi.py"
      }
    ]
  }
  